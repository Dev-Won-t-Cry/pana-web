    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
